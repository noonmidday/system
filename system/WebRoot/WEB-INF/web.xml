	<servlet-mapping>
		<servlet-name>login_ver</servlet-name>
		<url-pattern>/servlet/login_ver</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>show_product_list</servlet-name>
    <url-pattern>/servlet/product_info/show_product_list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>add_product_list</servlet-name>
    <url-pattern>/servlet/product_info/add_product_list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>sign_out</servlet-name>
    <url-pattern>/servlet/sign_out</url-pattern>
  </servlet-mapping>
