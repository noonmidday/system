		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<filter>
		<filter-name>Session_ver</filter-name>
		<filter-class>filter.Session_ver</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Session_ver</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>
